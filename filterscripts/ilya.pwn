// FUNCTION GET ITEM STORY
stock GetPosInMassive(itemId)
{
	switch itemId do
	{
		case 5780..5786: 
			return 2;

		case 7034: 
			return 1;

		case 7035: 
			return 19;

		case 7414:
			return 3;

		case 7425, 7701, 6090:
			return 4;

		case 7351:
			return 5;

		case 7345, 7346, 7347, 7348, 7349:
			return itemId - 7339;

		case 7281:
			return 11;

		case 6756: 
			return 12;

		case 6649: 
			return 13;

		case 6650:
			return 14;

		case 6161:
			return 15;

		case 6065:
			return 16;

		case 5736:
			return 17;

		case 5460:
			return 18;
			
		case 1810:
			return 20;

		default: { }
	}

	return 0;
}

public: @GetItemStory(playerid, itemId)
{
	SetPVarString(playerid, "GetItemStory", ItemStory[GetPosInMassive(itemId)]);

	return true;
}
