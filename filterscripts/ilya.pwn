public OnPlayerEnterDynamicArea(playerid, areaid)
{
	if areaid >= SEAT_SYSTEM_AREAS[0] && areaid <= SEAT_SYSTEM_AREAS[1] *then
		SetPlayerData(playerid, P_AREA_ID, areaid - SEAT_SYSTEM_AREAS[0]);
}

public OnPlayerLeaveDynamicArea(playerid, areaid)
{
	if areaid >= SEAT_SYSTEM_AREAS[0] && areaid <= SEAT_SYSTEM_AREAS[1] *then
		SetPlayerData(playerid, P_AREA_ID, -1);
}

public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
	if newkeys == KEY_ALT *then
	{
		if !GetPVarInt(playerid, "P_SEAT_STATUS") *then
		{
			help_id = GetPlayerData(playerid, P_AREA_ID);
			if !(help_id == -1) && IsPlayerInRangeOfPoint(playerid, 1.25, SEAT_SYSTEM_POS[help_id][Sx], SEAT_SYSTEM_POS[help_id][Sy], SEAT_SYSTEM_POS[help_id][Sz]) *then
			{
	            SetPlayerPos(playerid, SEAT_SYSTEM_POS[help_id][Sx], SEAT_SYSTEM_POS[help_id][Sy], SEAT_SYSTEM_POS[help_id][Sz]);
		        SetPlayerFacingAngle(playerid, SEAT_SYSTEM_POS[help_id][Aa]);
		        SetCameraBehindPlayer(playerid);
				ApplyAnimation(playerid, "PED", "SEAT_down", 4.1, false, false, false, true, 0, true);

				SetPVarInt(playerid, "P_SEAT_STATUS", true);

				SendPlayerData(playerid);
			}
		}
	}
	return true;
}

stock GetCountObjects()
{
	new count;
	for new i; i < 1000; i++ do
	{
		if IsValidObject(i) *then count ++;
	}
	return count;
}

stock GetPlayerLauncher(playerid)
{
	GetPlayerVersion(playerid, str, 24);
	return ( GetString(str, "Arizona PC") || GetString(str, "arizona-mobile")  ? 1:0 );
}

